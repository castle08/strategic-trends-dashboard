{
  "name": "strategic-trends-dashboard",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/trends",
      "dest": "/api/trends.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}